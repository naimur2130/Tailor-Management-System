@{
    ViewData["Title"] = "Completed Payments";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Completed Payments</h2>

<table id="CompletedPaymentsTable" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Invoice</th>
            <th>Customer</th>
            <th>Phone</th>
            <th>Amount</th>
            <th>Dress Type</th>
            <th>Order Status</th>
            <th>Created At</th>
            <th>Action</th>
        </tr>
    </thead>
</table>

@section Scripts {
    <script src="~/js/complete.js" asp-append-version="true"></script>
}
