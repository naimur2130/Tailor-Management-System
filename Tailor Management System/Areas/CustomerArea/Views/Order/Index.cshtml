@model IEnumerable<Tailor_Management_System.Models.Order>
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="container mt-5">
    <h2 class="mb-4 text-center">My Orders</h2>

    <div class="table-responsive shadow-sm rounded">
        <table class="table table-striped table-hover align-middle" id="MyOrdersTable">
            <thead class="table-dark text-center">
                <tr>
                    <th>Dress Type</th>
                    <th>Measurements</th>
                    <th>Time Duration</th>
                    <th>Status</th>
                    <th>Date</th>
                </tr>
            </thead>
            
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/myordertable.js" asp-append-version="true"></script>
}
